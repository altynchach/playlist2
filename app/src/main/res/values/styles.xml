<resources>
    <style name="GridStyle">
        <item name="android:background">@color/blue_sky</item>
    </style>

    <style name="BackButtonStyle">
        <item name="iconTint">@color/black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">0dp</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:scaleType">centerInside</item>
    </style>

    <style name="FrameLayoutStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_rowWeight">1</item>
        <item name="android:background">@drawable/button_background</item>
        <item name="android:layout_margin">@dimen/margin_standard</item>
        <item name="android:layout_gravity">start</item>
    </style>

    <style name="ButtonStyle" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:background">@color/transparent</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/header_text_size</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="iconTint">@color/black</item>
        <item name="iconPadding">@dimen/icon_padding</item>
        <item name="cornerRadius">@dimen/corner_radius</item>
    </style>

    <style name="SettingsTextViewStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:layout_weight">1</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>

    <style name="SettingsImageViewStyle">
        <item name="android:layout_width">@dimen/layout_width_50dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">end</item>
    </style>

    <style name="SettingsLinearLayoutStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginTop">@dimen/margin_top</item>
    </style>

    <style name="search">
        <item name="tint">@color/search_text</item>
        <item name="android:textColorHint">@color/search_text</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/search_text</item>
        <item name="android:drawableTint">@color/search_text</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>

    <style name="errorText">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>

    <style name="reloadButton">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>

    <style name="header_page">
        <item name="android:textColor">@color/search_text</item>
        <item name="android:textSize">@dimen/header_text_size2</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="search_image_view">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">102dp</item>
    </style>

    <style name="MainButtonStyle" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:insetBottom">0dp</item>
        <item name="iconGravity">textStart</item>
        <item name="iconTint">@color/black</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/header_text_size2</item>
        <item name="cornerRadius">@dimen/button_radius</item>
        <item name="iconPadding">6dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>

    <style name="ClearSearchButtonStyle" parent="Widget.MaterialComponents.Button.UnelevatedButton">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_gravity">center</item>
    <item name="android:backgroundTint">@color/search_text</item>
    <item name="android:layout_marginTop">@dimen/margin_top</item>
    <item name="android:textColor">@color/black_dark</item>
    <item name="android:textSize">@dimen/error_button_text</item>
    <item name="cornerRadius">@dimen/error_button_radius</item>
    <item name="android:textAllCaps">false</item>
    <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>

    <style name="HistorySearchText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:textSize">@dimen/error_search_text</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/search_text</item>
        <item name="android:layout_marginTop">@dimen/button_radius</item>
    </style>

    <style name="TextOnPlayerRight">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:layout_marginBottom">18dp</item>
        <item name="layout_constrainedWidth">true</item>
        <item name="layout_constraintHorizontal_bias">1</item>
    </style>

    <style name="TextOnPlayerLeft">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textFontWeight">400</item>
        <item name="fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:layout_marginBottom">18dp</item>
    </style>

    <style name="NothingFoundImage">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@drawable/img_nothing_found_light</item>
    </style>

    <style name="SettingsBackgroundColorStyle">
        <item name="android:background">@color/white_black</item>
    </style>

    <style name="BackButtonSettingsStyle">
        <item name="android:tint">@color/search_text</item>
    </style>

    <style name="tabLayoutStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="tabTextColor">@color/search_text</item>
        <item name="tabIndicatorColor">@color/search_text</item>
        <item name="tabBackground">@color/white_black</item>
    </style>

    <style name="MineCustomTabText" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">@dimen/error_button_text</item>
        <item name="fontFamily">@font/ys_display_medium</item>
        <item name="textAllCaps">false</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:letterSpacing">0</item>
    </style>

    <style name="TextSearchProblem">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">@color/search_text</item>
        <item name="android:textSize">19sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>

    <style name="NavigationView">
        <item name="itemIconTint">@drawable/bottom_nav_icon_selector</item>
        <item name="itemTextColor">@drawable/bottom_nav_icon_selector</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="ResetButtonSearch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/search_text</item>
        <item name="cornerRadius">@dimen/error_button_radius</item>
        <item name="android:text">@string/reload</item>
        <item name="android:textColor">@color/white_black</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/error_button_text</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginTop">@dimen/size334</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
    </style>


    <style name="editText" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:textColorHint">@drawable/text_selector</item>
        <item name="boxStrokeColor">@drawable/edit_text_selector</item>

    </style>

    <style name="EditTextStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:padding">@dimen/button_radius</item>
        <item name="android:textColorHint">@color/black_main</item>
        <item name="android:layout_marginHorizontal">@dimen/button_radius</item>
        <item name="android:background">@drawable/rounded_rectangle_gray</item>
    </style>

    <style name="BottomSheetStyle">
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="BottomSheetViewStyle">
        <item name="android:backgroundTint">@color/gray</item>
    </style>

    <style name="BottomSheetTitleStyle">
        <item name="android:textColor">@color/black_main</item>
    </style>

    <style name="BottomSheetButton">
        <item name="android:textColor">@color/white</item>
        <item name="android:backgroundTint">@color/black_main</item>
    </style>

    <style name="PlaylistTitleStyle">
        <item name="android:textColor">@color/black_main</item>
    </style>

</resources>